# AI Provider API Keys
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here_optional

# Redis Configuration
REDIS_URL=redis://redis:6379/0
REDIS_PASSWORD=your_redis_password_optional

# Application Settings
ENVIRONMENT=production
API_HOST=localhost:5000
PORT=5000
LOG_LEVEL=INFO

# Monitoring
GRAFANA_USER=admin
GRAFANA_PASSWORD=change_this_password

# Rate Limiting (per tier)
# Free tier: 100 requests/hour, 1000/day
# Starter tier: 1000 requests/hour, 10000/day
# Professional tier: 10000 requests/hour, 100000/day
# Enterprise tier: unlimited

# Security
SECRET_KEY=your_secret_key_for_jwt
ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com

# Optional: Multi-provider configuration
PRIMARY_PROVIDER=gemini
FALLBACK_PROVIDER=openai
CIRCUIT_BREAKER_THRESHOLD=5
CIRCUIT_BREAKER_TIMEOUT=60